MODULE SaleSanitation;

REQUIRE Sanitation, SaleCertificate;

NAMESPACE Sale;

@defineDocumentCertificateObject(sanitation, invoice, i, 'ГГР');
@defineDocumentCertificateConstraint(sanitation, invoice);
