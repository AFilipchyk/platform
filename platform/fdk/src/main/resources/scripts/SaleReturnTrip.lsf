MODULE SaleReturnTrip;

REQUIRE SaleReturnOrder,
        Trip;

NAMESPACE SaleReturn;

EXTEND CLASS order : orderTrip;
EXTEND CLASS orderDetail : orderTripDetail;

numberOrderTrip(order) += numberOrder(order);
seriesOrderTrip(order) += seriesOrder(order);
dateOrderTrip(order) += dateOrder(order);
grossWeightOrderTrip(order) += grossWeightOrder(order);

quantityOrderTripDetail(orderDetail) += quantityOrderDetail(orderDetail);