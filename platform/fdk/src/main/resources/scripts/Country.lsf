MODULE Country;

REQUIRE System, DefaultData, I18n, Currency;

CLASS country 'Страна' : named, multiLanguageNamed;

TABLE countryDate(country, DATE);
TABLE country(country);

nameOriginCountry 'Наименование (ориг.)' (country) = DATA ISTRING[50] (country) IN baseGroup; //logics.country.name.origin.country
sidCountry 'Код страны' (country) = DATA INTEGER (country) IN baseGroup; // logics.country.key
countrySID 'Страна' (sid) =  GROUP UNIQUE country BY sidCountry(country);  // logics.country

sidOrigin2Country 'Код 2 знака (ориг.)' (country) = DATA STRING[2] (country) IN baseGroup MINCHARWIDTH 15; // logics.country.sid.origin.2.country
sidOrigin3Country 'Код 3 знака (ориг.)' (country) = DATA STRING[3] (country) IN baseGroup MINCHARWIDTH 15; // logics.country.sid.origin.3.country

currencyCountry 'Валюта' (country) = DATA currency (country); // logics.country.currency
nameCurrencyCountry 'Валюта' (country) = name(currencyCountry(country)) IN baseGroup PREFCHARWIDTH 10;

generateDatesCountry 'Генерировать выходные' (country) = DATA BOOLEAN (country); // logics.day.generate.days.off

residentCountry 'Отечественный производитель' (country) = DATA BOOLEAN (country) IN baseGroup MINCHARWIDTH 15; // logics.country.resident.country

isDayOffCountryDate 'Выходной' (country, date) = DATA BOOLEAN (country, DATE) IN baseGroup; // logics.day.off

countryName(name) = GROUP UNIQUE country BY name(country) WHERE country IS country;

defaultCountry 'Страна по умолчанию' () = DATA country (); // logics.country.default.country
nameDefaultCountry 'Страна по умолчанию' () = name(defaultCountry()) PREFCHARWIDTH 30; // logics.country.default.country

FORM daysOffForm 'Выходные дни' // logics.days.off
OBJECTS country 'Страна' INIT PANEL, day=DATE 'День', newDate=DATE 'Дата' FIXED PANEL
PROPERTIES(country) nameOriginCountry, sidCountry, sidOrigin2Country, sidOrigin3Country, nameCurrencyCountry, residentCountry
PROPERTIES OBJVALUE(day), isDayOffCountryDate(country, day), OBJVALUE(newDate), isDayOffCountryDate(country, newDate)
FILTERS isDayOffCountryDate(country, day)
;

DESIGN daysOffForm FROM DEFAULT {
    day.grid {
        fillVertical = 3;
    }
}

EXTEND FORM options
    PROPERTIES() nameDefaultCountry
;

EXTEND DESIGN options {
    commons {
        ADD PROPERTY(nameDefaultCountry);
    }
}

languageCountry = DATA language (country);
nameLanguageCountry 'Язык' (country) = name(languageCountry(country)) MINCHARWIDTH 5 PREFCHARWIDTH 10 MAXCHARWIDTH 15;

FORM country 'Страна'
    OBJECTS c = country FIXED PANEL

    PROPERTIES(c) name, nameOriginCountry, sidCountry, sidOrigin2Country, sidOrigin3Country,
                  residentCountry, nameCurrencyCountry, nameLanguageCountry

    OBJECTS l = language
    PROPERTIES name(l) READONLY, languageName(c, l)

    EDIT country OBJECT c
;

FORM countries 'Страны'
    OBJECTS c = country

    PROPERTIES(c) READONLY name, nameOriginCountry, sidCountry, sidOrigin2Country, sidOrigin3Country,
                           residentCountry, nameCurrencyCountry, nameLanguageCountry

    PROPERTIES(c) ADDFORM, EDITFORM, delete
    ORDER BY name

    DIALOG country OBJECT c
;

NAVIGATOR {
    catalogElement {
        ADD countries;
        ADD daysOffForm;
    }
}

// ------------------------------------------------ Стандартные значения --------------------------------------- //

loadDefaultCountry 'Добавить страны' = ACTION (string, number, stringA, stringB, stringC, shortCurrency) {//, value) {
    ADDOBJ country;
    FOR c == addedObject() DO {
       SET name(c) <- string AS STRING[110];
       SET sidCountry(c) <- number AS INTEGER;
       SET nameOriginCountry(c) <- stringA AS STRING[50];
       SET sidOrigin2Country(c) <- stringB AS STRING[2];
       SET sidOrigin3Country(c) <- stringC AS STRING[3];
       SET currencyCountry(c) <- currencyShortName(shortCurrency);
    }
}

