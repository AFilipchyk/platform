<?xml version="1.0"?>
<project name="LS Fusion Build Utils">
    <target name="recursiveCompile">
        <exec dir="${basedir}" executable="cmd" failonerror="true">
            <arg value="/c"/>
            <arg value="mvn"/>
            <!--<arg value="-X"/>-->
            <arg value="--also-make" />
            <arg value="--projects" />
            <arg value="by.luxsoft.platform:${artifactId}" />
            <arg value="compile" />
        </exec>
    </target>

    <target name="quickCompile">
        <exec dir="${basedir}/${artifactId}" executable="cmd" failonerror="true">
            <arg value="/c"/>
            <arg value="mvn"/>
            <arg value="compile" />
            <arg value="-Dquickbuild" />
        </exec>
    </target>

    <target name="recursiveTestCompile">
        <exec dir="${basedir}" executable="cmd" failonerror="true">
            <arg value="/c"/>
            <arg value="mvn"/>
            <arg value="-Dmaven.test.skip=false"/>
            <!--<arg value="-X"/>-->
            <arg value="--also-make" />
            <arg value="--projects" />
            <arg value="by.luxsoft.platform:${artifactId}" />
            <arg value="test-compile" />
        </exec>
    </target>

    <target name="quickTestCompile">
        <exec dir="${basedir}/${artifactId}" executable="cmd" failonerror="true">
            <arg value="/c"/>
            <arg value="mvn"/>
            <arg value="-Dmaven.test.skip=false"/>
            <arg value="test-compile" />
            <arg value="-Dquickbuild" />
        </exec>
    </target>

    <target name="base-recursiveCompile">
        <antcall target="recursiveCompile">
            <param name="artifactId" value="base"/>
        </antcall>
    </target>

    <target name="base-quickCompile">
