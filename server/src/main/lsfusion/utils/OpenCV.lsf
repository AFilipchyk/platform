MODULE OpenCV;

//need tess4j in pom.xml
imageToString INTERNAL  'lsfusion.utils.utils.ImageToStringOpenCVActionProperty' (IMAGEFILE, VARSTRING[255], INTEGER, INTEGER);
imageToStringResult = DATA LOCAL TEXT();

imageToStringAction() {
    INPUT f = IMAGEFILE DO  {
        imageToString(f, 'e:/work/tessdata', 1, 3);
        MESSAGE imageToStringResult();
    }
}