<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:sk='urn:import:skolkovo.gwt.base.client.ui'>
    <g:VerticalPanel width="100%">
        <g:cell horizontalAlignment="center">
            <g:VerticalPanel width="800" styleName="mainPane" spacing="5">
                <sk:ToolbarPanel showProfileButton="false" />

                <g:HTMLPanel>
                    <g:Image styleName="floatRight" url="images/logo.png" altText="Logo"/>
                    <hr size="5" color="#000000"/>
                    <center><h2><span ui:field="titleSpan"/></h2></center>
                    <table>
                        <tr>
                          <td align="left"><b><span ui:field="nameLabelSpan" /></b></td><td align="left"><span ui:field="nameSpan"/></td>
                        </tr>
                        <tr>
                          <td></td>
                          <td align="left" style="font-size: 10pt"><i><g:Label ui:field="logoffNotice"/></i></td>
                          <td align="left" style="font-size: 10pt"><i><g:Anchor href="javascript:;" ui:field="logoffLink" /></i></td>
                        </tr>
                        <tr>
                          <td align="left"><b><span ui:field="emailLabelSpan"/></b></td><td align="left"><span ui:field="emailSpan"/></td>
                        </tr>
                    </table>

                    <g:CheckBox ui:field="cbShowUnvoted"/>
                    <c:CellTable width="100%" ui:field="table"/>
                    <g:Label width="100%" visible="false" ui:field="lbEmptyList" />
                </g:HTMLPanel>
            </g:VerticalPanel>
        </g:cell>
    </g:VerticalPanel>
</ui:UiBinder>