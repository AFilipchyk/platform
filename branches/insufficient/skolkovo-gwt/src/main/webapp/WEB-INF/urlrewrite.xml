        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule>
        <from>(.*)/logoff.html(.*)$</from>
        <to type="forward">$1/logoff.jsp$2</to>
    </rule>
</urlrewrite>